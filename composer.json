{
    "name": "elcweb/angularjs-bundle",
    "type": "symfony-bundle",
    "description": "HTML is great for declaring static documents, but it falters when we try to use it for declaring dynamic views in web-applications. AngularJS lets you extend HTML vocabulary for your application. The resulting environment is extraordinarily expressive, readable, and quick to develop.",
    "license": "mit",
    "authors": [
        {
            "name": "Etienne Lachance",
            "email": "el@elcweb.ca"
        }
    ],
    "require": {
        "php"                      : ">=5.3.3",
        "symfony/framework-bundle" : ">=2.1,<3.0-dev",
        "components/angular.js": "1.3.*",
        "angular/strap": "v2.2.4",
        "angular/notify": "v2.5.0",
        "angular/input-mask": "v1.5.1"
    },
    "autoload": {
        "psr-0": { "Elcweb\\AngularJSBundle": "" }
    },
    "minimum-stability": "dev",
    "target-dir": "Elcweb/AngularJSBundle",
    "repositories": [
        {
            "type"   : "package",
            "package": {
                "name"   : "angular/strap",
                "version": "v2.2.4",
                "source"   : {
                    "url" : "https://github.com/mgcrea/angular-strap.git",
                    "type": "git",
                    "reference": "v2.2.4"
                }
            }
        },
        {
            "type"   : "package",
            "package": {
                "name"   : "angular/notify",
                "version": "v2.5.0",
                "source"   : {
                    "url" : "https://github.com/cgross/angular-notify.git",
                    "type": "git",
                    "reference": "v2.5.0"
                }
            }
        },
        {
            "type"   : "package",
            "package": {
                "name"   : "angular/input-mask",
                "version": "v1.5.1",
                "source"   : {
                    "url" : "https://github.com/assisrafael/angular-input-masks.git",
                    "type": "git",
                    "reference": "v1.5.1"
                }
            }
        }
    ]
}
